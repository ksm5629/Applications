<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime targetFramework="4.5"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IService" />
        <binding name="BasicHttpBinding_IService1" />
        <binding name="BasicHttpBinding_IService2" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost/CryptoServ/Service.svc" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IService" contract="CryptoServiceRef.IService"
        name="BasicHttpBinding_IService" />
      <endpoint address="http://vhost3.cs.rit.edu/SortServ/Service.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService1"
        contract="SortServiceRef.IService" name="BasicHttpBinding_IService1" />
      <endpoint address="http://vhost3.cs.rit.edu/SortServ/Service.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService2"
        contract="SortService.IService" name="BasicHttpBinding_IService2" />
    </client>
  </system.serviceModel>
</configuration>